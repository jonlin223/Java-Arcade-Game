<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.4" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane fx:id="background" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="500.0">
         <children>
            <Button fx:id="selectAdvanced" layoutX="345.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleSelectAdvanced" prefHeight="26.0" prefWidth="88.0" text="Advanced" />
            <Button fx:id="selectBoulders" layoutX="205.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleSelectBoulders" prefHeight="26.0" prefWidth="88.0" text="Boulders" />
            <Button fx:id="selectMaze" layoutX="65.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleSelectMaze" prefHeight="26.0" prefWidth="88.0" text="Maze" />
            <Label fx:id="title" alignment="CENTER" layoutX="7.0" layoutY="26.0" prefHeight="50.0" prefWidth="487.0" text="Death in the Catacombs" textFill="#ff00ab">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
            <Button fx:id="exitButton" layoutX="345.0" layoutY="246.0" mnemonicParsing="false" onAction="#handleExitButton" prefHeight="26.0" prefWidth="88.0" text="Exit Game" />
            <Label fx:id="title1" alignment="CENTER" layoutX="139.0" layoutY="159.0" prefHeight="26.0" prefWidth="223.0" text="Level Select" textFill="#ff00ab">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <Button fx:id="selectPortals" layoutX="65.0" layoutY="246.0" mnemonicParsing="false" onAction="#handleSelectPortals" prefHeight="26.0" prefWidth="88.0" text="Portals" />
            <Button fx:id="selectInfo" layoutX="205.0" layoutY="246.0" mnemonicParsing="false" onAction="#handleSelectInfo" prefHeight="26.0" prefWidth="88.0" text="Game Info" />
         </children>
      </Pane>
   </children>
</VBox>
